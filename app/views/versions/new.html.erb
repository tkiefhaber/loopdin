<%= form_for [@user, @project, @version], :html => {class: "form"} do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <div class='form-actions form-actions-padding'>
    <%= f.submit %>
  </div>

<% end %>
